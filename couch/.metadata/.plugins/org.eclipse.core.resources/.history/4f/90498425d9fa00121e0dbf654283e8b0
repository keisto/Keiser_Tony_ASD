$.couch.urlPrefix = "http://localhost:5984";

$.couch.info({
    success: function(data) {
        console.log(data);
    }
});

$.couch.alldDbs ({
    success: function(data) {
        console.log(data);
    }
});

$(document).on('pageinit', '#home', function(){
   // CouchDB Code
   $.couch.db(""), {
   		success:function(data){
   			console.log(data);
   		}
   };
});








